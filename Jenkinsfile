pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello World build'
            }
        }
        stage('test') {
            steps {
                echo 'Hello World test'
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello World deploy'
            }
        }
        stage('extra-stage') {
            steps {
                echo 'Hello World extra stage in develop only'
            }
        }

    }
}

